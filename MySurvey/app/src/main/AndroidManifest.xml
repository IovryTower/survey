<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.qin.MySurvey">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.qin.MySurvey.UnlockView"></activity>
        <activity android:name="com.qin.MySurvey.Report" />
        <activity android:name="com.qin.MySurvey.Show_json" />
        <activity android:name="com.qin.MySurvey.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.qin.MySurvey.SActivity" />
        <activity android:name="com.qin.MySurvey.Question_one" />
        <activity android:name="com.qin.MySurvey.TimeActivity" />
        <activity
            android:name="com.king.zxing.CaptureActivity"
            android:screenOrientation="portrait" />
        <activity android:name="com.qin.MySurvey.EasyCaptureActivity" />
    </application>

</manifest>